{
  "name": "treblle-sdk",
  "version": "1.0.0",
  "description": "Official Treblle SDK for NodeJS applications",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "clean": "rimraf dist",
    "lint": "eslint 'src/**/*.ts'",
    "lint:fix": "eslint 'src/**/*.ts' --fix",
    "format": "prettier --write 'src/**/*.ts'",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "prebuild": "npm run clean",
    "prepare": "npm run build",
    "prepublishOnly": "npm run test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/treblle/treblle-sdk-nodejs.git"
  },
  "keywords": [
    "treblle",
    "api",
    "monitoring",
    "logging",
    "sdk",
    "nodejs",
    "express",
    "nestjs",
    "koa"
  ],
  "engines": {
    "node": ">=14.0.0"
  },
  "author": "Treblle Inc.",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/treblle/treblle-sdk-nodejs/issues"
  },
  "homepage": "https://github.com/treblle/treblle-sdk-nodejs#readme",
  "dependencies": {},
  "peerDependencies": {
    "express": ">=4.16.0"
  },
  "devDependencies": {
    "@nestjs/common": "^9.0.0",
    "@nestjs/core": "^9.0.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.1",
    "@types/node": "^18.16.3",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "@typescript-eslint/parser": "^5.59.2",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-prettier": "^4.2.1",
    "express": "^4.18.2",
    "jest": "^29.5.0",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.4"
  },
  "files": [
    "dist/**/*"
  ]
}