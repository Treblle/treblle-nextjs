{
  "name": "@treblle/next",
  "version": "1.0.0",
  "description": "Official Treblle SDK for Next.js applications",
  "main": "module-wrapper.js",
  "module": "./dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./module-wrapper.js",
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./integrations": {
      "require": "./dist/integrations/index.js",
      "import": "./dist/integrations/index.js",
      "types": "./dist/integrations/index.d.ts"
    },
    "./integrations/nextjs": {
      "require": "./dist/integrations/nextjs.js",
      "import": "./dist/integrations/nextjs.js",
      "types": "./dist/integrations/nextjs.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "prepare": "npm run build",
    "dev": "tsc --watch",
    "clean": "rm -rf dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Treblle/treblle-js.git"
  },
  "keywords": [
    "api",
    "monitoring",
    "nextjs",
    "next.js",
    "app-router",
    "pages-router",
    "middleware",
    "serverless",
    "edge-runtime",
    "treblle",
    "analytics",
    "telemetry"
  ],
  "author": "Treblle",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Treblle/treblle-js/issues"
  },
  "homepage": "https://github.com/Treblle/treblle-js#readme",
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^18.19.120",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "eslint": "^9.32.0",
    "jest": "^29.7.0",
    "next": "^15.4.4",
    "ts-jest": "^29.1.2",
    "typescript": "^5.0.4"
  },
  "peerDependencies": {
    "next": ">=13.0.0"
  },
  "dependencies": {},
  "engines": {
    "node": ">=18.14.0"
  },
  "sideEffects": false,
  "files": [
    "dist",
    "module-wrapper.js",
    "README.md",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "public"
  }
}
