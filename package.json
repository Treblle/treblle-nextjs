{
  "name": "treblle-js",
  "version": "1.0.0",
  "description": "Official Treblle SDK for NodeJS applications",
  "main": "module-wrapper.js",
  "module": "dist/esm/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./module-wrapper.js",
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./integrations": {
      "require": "./dist/integrations/index.js",
      "import": "./dist/integrations/index.js",
      "types": "./dist/integrations/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "prepare": "npm run build",
    "express": "npm --prefix ./examples/express run dev",
    "nestjs": "npm --prefix ./examples/nestjs run dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Treblle/treblle-js.git"
  },
  "keywords": [
    "api",
    "monitoring",
    "nodejs",
    "express",
    "nestjs",
    "treblle"
  ],
  "author": "Treblle",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Treblle/treblle-js/issues"
  },
  "homepage": "https://github.com/Treblle/treblle-js#readme",
  "optionalDependencies": {
    "@nestjs/common": "^8.0.0",
    "rxjs": "^7.0.0"
  },
  "devDependencies": {
    "@nestjs/common": "^11.1.0",
    "@ngrx/operators": "^19.1.0",
    "@prettier/plugin-xml": "^3.4.1",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.14",
    "@types/node": "^18.15.11",
    "jest": "^29.7.0",
    "prettier-plugin-packagejson": "^2.5.11",
    "rxjs": "^7.8.2",
    "ts-jest": "^29.3.2",
    "typescript": "^5.0.4"
  },
  "peerDependencies": {
    "express": ">=4.0.0"
  }
}