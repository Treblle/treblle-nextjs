{
  "name": "treblle-js",
  "version": "1.0.0",
  "description": "Official Treblle SDK for NodeJS applications",
  "main": "module-wrapper.js",
  "module": "dist/esm/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./module-wrapper.js",
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./integrations": {
      "require": "./dist/integrations/index.js",
      "import": "./dist/integrations/index.js",
      "types": "./dist/integrations/index.d.ts"
    },
    "./integrations/nextjs": {
      "require": "./dist/integrations/nextjs.js",
      "import": "./dist/integrations/nextjs.js",
      "types": "./dist/integrations/nextjs.d.ts"
    },
    "./integrations/express": {
      "require": "./dist/integrations/express.js",
      "import": "./dist/integrations/express.js",
      "types": "./dist/integrations/express.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "prepare": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Treblle/treblle-js.git"
  },
  "keywords": [
    "api",
    "monitoring",
    "nodejs",
    "express",
    "nestjs",
    "treblle"
  ],
  "author": "Treblle",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Treblle/treblle-js/issues"
  },
  "homepage": "https://github.com/Treblle/treblle-js#readme",
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/node": "^18.19.120",
    "next": "^15.4.4",
    "typescript": "^5.0.4"
  },
  "peerDependencies": {
    "express": ">=4.0.0"
  },
  "dependencies": {
    "@nestjs/common": "^11.1.5",
    "@nestjs/core": "^11.1.5",
    "dotenv": "^17.2.1"
  }
}
